{
  "manifest_version": 3,
  "name": "Schedule Hack",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://tanrend.elte.hu/*"],
      "run_at": "document_idle",
      "js": ["contentScript.js"]
    }
  ]
}
